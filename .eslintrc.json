{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": "warn",
        "no-const-assign": "warn",
        "no-mixed-spaces-and-tabs": "warn",
        "no-multi-spaces": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "error",
        "no-unused-vars": "warn",
        "no-extra-semi": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}